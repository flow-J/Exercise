#include <iostream>
int main()
{
    std::cout << "Hello"::endl;
    std::cout << ","::endl;
    std::cout << "world"::endl;
    std::cout << "."::endl;
    int v1 = 1;
    int v2 = 1;
    std::cin >> v1;
    std::cin >> v2;
    std::cout << "I got a number" std::endl;
    std::cout << v1 std::endl;
    std::cout << "+" std::endl;
    std::cout << v2 std::endl;
    std::cout << "=" std::endl;
    std::cout << v1 + v2 std::endl;
    std::cout << "Well, I guess i know too much."std::endl;
}
